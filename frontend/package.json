{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --no-lint",
    "build:turbo": "next build --turbopack",
    "build:analyze": "ANALYZE=true npm run build",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "type-check": "tsc --noEmit",
    "analyze": "npm run build && npx @next/bundle-analyzer",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "cypress:ci": "cypress run --browser chrome --headless",
    "test:e2e": "start-server-and-test dev http://localhost:3000 cypress:run",
    "test:all": "npm run test:ci && npm run cypress:ci",
    "vercel-build": "npm run lint && npm run type-check && npm run build",
    "postinstall": "echo 'Installation complete'"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-web": "^0.49.0",
    "@opentelemetry/exporter-otlp-http": "^0.26.0",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@opentelemetry/sdk-trace-web": "^2.0.1",
    "@opentelemetry/semantic-conventions": "^1.36.0",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.45.4",
    "@tanstack/react-query": "^5.85.6",
    "@tanstack/react-query-devtools": "^5.85.6",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "immer": "^10.1.1",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-error-boundary": "^6.0.0",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1",
    "zundo": "^2.3.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@next/bundle-analyzer": "^15.5.2",
    "@pact-foundation/pact": "^15.0.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20.19.13",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19",
    "cypress": "^15.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "jest": "^30.1.1",
    "jest-environment-jsdom": "^30.1.1",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.9.2",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
