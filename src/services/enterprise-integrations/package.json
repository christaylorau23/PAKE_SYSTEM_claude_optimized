{
  "name": "@pake/enterprise-integrations",
  "version": "1.0.0",
  "description": "PAKE System - Enterprise Tool Integration Suite with 50+ Platform Connectors",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "clean": "rm -rf dist",
    "docker:build": "docker build -t pake-enterprise-integrations .",
    "docker:run": "docker run -p 3007:3007 pake-enterprise-integrations",
    "migrate": "ts-node src/database/migrate.ts",
    "seed": "ts-node src/database/seed.ts"
  },
  "keywords": [
    "enterprise-integration",
    "microsoft-365",
    "google-workspace",
    "slack",
    "teams",
    "api-gateway",
    "data-synchronization"
  ],
