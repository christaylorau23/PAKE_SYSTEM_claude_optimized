  "description": "PAKE System - Autonomous Agent Framework with Multi-Agent Orchestration",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc && tsc-alias",
